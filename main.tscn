[sub_resource type="GDScript" id="GDScript_h2yge"]
script/source = "extends Node

var ws := WebSocketPeer.new()
var ax = 0
var ay = 0
var az = 0

func _ready():
	print(\"connecting to python server\")
	ws.connect_to_url(\"ws://localhost:8766\")
	set_process(true)
	
func _process(delta:float) -> void:
	ws.poll()
	
	#check connection
	if ws.get_ready_state() != WebSocketPeer.STATE_OPEN:
		return
		
	#read packets
	while ws.get_available_packet_count() > 0:
		var msg: String = ws.get_packet().get_string_from_utf8()
		
		var ok: bool
		var data: Dictionary = {}

		# JSON.parse_string returns Variant, use JSON.parse_string(msg).result directly
		var parse_result = JSON.parse_string(msg)

		if parse_result.has(\"error\") and parse_result[\"error\"] != OK:
			print(\"JSON parse error\")
			continue

		# parse_result.result is the dictionary
		if parse_result.has(\"result\"):
			data = parse_result[\"result\"]

		if data.has(\"phone\"):
			ax = float(data[\"phone\"][\"ax\"])
			ay = float(data[\"phone\"][\"ay\"])
			az = float(data[\"phone\"][\"az\"])
"

